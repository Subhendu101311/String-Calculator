public class StringCalculator {
   public int add(string numbers) {
     int i = 0;
     int n = number.length();
     int sum = 0;
     for(i=0, i<n; i++){
     if(number.charAt(i) >= 0 && number.charAt(i) <3){
     sum+= number.charAt(i);
     }
     }
   }

   public static void main(String []args) {
   
   int result = 0;
      StringCalculator sc = new StringCalculator();
      
      result = sc.add("a0123");
      System.out.println(result);
   }
}
